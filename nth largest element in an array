import java.util.Scanner;
import java.util.Arrays;
public class Java{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        System.out.println("enter size:");
        int n=sc.nextInt();
        int[] a=new int[n];
        for(int i=0;i<n;i++){
            a[i]=sc.nextInt();
        }
        System.out.println("enter m value:");
        int m=sc.nextInt();
        Arrays.sort(a);
        int max=a[n-m];
        System.out.println(max);
    }
}
